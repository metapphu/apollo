{
    "name": "metapphu/apollo",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Author",
            "name": "Metapp",
            "email": "info@metapp.hu"
        }
    ],
    "autoload": {
        "psr-4": {
            "Metapp\\Apollo\\": "src/"
        }
    },
    "require": {
        "php": "7.2.5",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-reflection": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-gd": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "doctrine/doctrine-module": "^4.1",
        "league/container": "2.4.1",
        "monolog/monolog": "^2.3",
        "guzzlehttp/psr7": "^2.1",
        "league/route": "3.1.0",
        "twig/twig": "^3.3",
        "doctrine/orm": "^2.10",
        "beberlei/doctrineextensions": "^1.3",
        "firebase/php-jwt": "^5.5",
        "phpmailer/phpmailer": "^6.5",
        "php-curl-class/php-curl-class": "^9.5",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-mvc-i18n": "^1.1",
        "guzzlehttp/guzzle": "^7.4",
        "gedmo/doctrine-extensions": "3.6.0"
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "platform": {
            "php": "7.2.5"
        }
    }
}
