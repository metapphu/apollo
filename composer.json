{
    "name": "metapphu/apollo",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Author",
            "name": "Metapp",
            "email": "info@metapp.hu"
        }
    ],
    "autoload": {
        "psr-4": {
            "Metapp\\Apollo\\": "src/"
        }
    },
    "require": {
        "php": ">=8.3",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-reflection": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-gd": "*",
        "ext-exif": "*",
        "ext-redis": "*",
        "ext-fileinfo": "*",
        "doctrine/doctrine-module": "^6.3",
        "doctrine/orm": "^3.3",
        "gedmo/doctrine-extensions": "3.19",
        "laminas/laminas-diactoros": "^3.5",
        "laminas/laminas-mvc-i18n": "^1.9",
        "laminas/laminas-i18n": "^2.29",
        "laminas/laminas-servicemanager": "^3.23",
        "laminas/laminas-httphandlerrunner": "^2.11",
        "league/container": "^4.2",
        "league/route": "^5.1",
        "monolog/monolog": "^3.8",
        "guzzlehttp/psr7": "^2.7",
        "twig/twig": "^3.20",
        "beberlei/doctrineextensions": "^1.5",
        "firebase/php-jwt": "^6.11",
        "guzzlehttp/guzzle": "^7.9",
        "cherif/inertia-psr15": "^0.4.0",
        "phpmailer/phpmailer": "^6.9",
        "php-curl-class/php-curl-class": "^11.0",
        "fullpipe/twig-webpack-extension": "^4.0",
        "voku/anti-xss": "^4.1",
        "symfony/cache": "^5.4",
        "doctrine/cache": "^2.2",
        "intervention/image": "^3.6"
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1"
    }
}
